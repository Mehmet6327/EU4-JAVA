package day38_Inheritance_Part2;

public class Dog  extends Animal {

	private int eyes;
	private int legs;
	private int tail;
	private int teeth;
	private String coat;
	public Dog(int eyes, int legs, int tail, int teeth, String coat) {
		super();
		this.eyes = eyes;
		this.legs = legs;
		this.tail = tail;
		this.teeth = teeth;
		this.coat = coat;
	}
	

	

	
	
	
	
	
	
	
	
	
	
}
